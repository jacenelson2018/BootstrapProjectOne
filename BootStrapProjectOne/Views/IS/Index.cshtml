<div class="container">
    @foreach (var item in Model)
    {
        <div class="col-md-4 portfolio-item">
            <a href="@Url.Action("SingleItem", "Portfolio")">
                <img src="http://placehold.it/700x400" class="img-responsive">
            </a>
            <a href="@Url.Action("SingleItem", "Portfolio")">

                <h3>
                    @Html.DisplayFor(modelItem => item.fName) @Html.DisplayFor(modelItem => item.lName)
                </h3>
            </a>
            <p>
                @Html.DisplayFor(modelItem => item.Company): @Html.DisplayFor(modelItem => item.Experience)
            </p>
            <p>
                @Html.ActionLink("Edit", "EditStudent", new { sCode = item.StudID }) |
                @Html.ActionLink("Details", "Details", new { sCode = item.StudID }) |
                @Html.ActionLink("Delete", "DeleteStudent", new { sCode = item.StudID })
            </p>
        </div>
    }
</div>






