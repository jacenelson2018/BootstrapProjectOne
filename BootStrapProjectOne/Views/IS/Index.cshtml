@model IEnumerable<BootStrapProjectOne.Models.Student>

@{
    ViewBag.Title = "Index";
}




<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">
                Information Systems
                <small>Showcase Your Work</small>
            </h1>
            <ol class="breadcrumb">
                <li>
                    @Html.ActionLink("Home", "Index", "Home")
                </li>
                <li>

                    @Html.ActionLink("Create New", "AddStudent")

                </li>
            </ol>
        </div>
    </div>
</div>
